@model ReportResult

@{
    ViewBag.Title = Model.Title;
}

<h2>@Model.Title</h2>

<link href="@Url.Content("~/Content/DataTables-1.9.4/media/css/jquery.dataTables.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/DataTables-1.9.4/media/css/jquery.dataTables_themeroller.css")" />
<style>
    @Model.Css
</style>


<table id="report"></table>

@section scripts
{
    <script src="@Url.Content("~/Scripts/DataTables-1.9.4/media/js/jquery.dataTables.min.js")"></script>

    <script type="text/javascript">
        $(document).ready(function() {
            var columns = @Html.Raw(Model.Columns);
            var data = @Html.Raw(Model.Data);
            @Html.Raw(Model.JavaScript)
        });
    </script>
}
