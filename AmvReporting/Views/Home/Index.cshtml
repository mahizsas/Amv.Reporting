@model IEnumerable<Report>

@{
    ViewBag.Title = "Home Page";
}

<h2>All Reports</h2>

<div class="row">
    <div class="col-md-9">

        <ul>
            @foreach (var item in Model)
            {
                <li>@Html.ActionLink(item.Title, MVC.Home.Report(item.Id))</li>
            }
        </ul>
    </div>

</div>