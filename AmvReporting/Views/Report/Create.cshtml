@model ReportDetailsViewModel

@{
    ViewBag.Title = "Create New Graph";
}
@section scripts{
    @Html.Partial(MVC.Report.Views._CommonJavaScript)
}
@section css{
    
}


<h2>Create New Graph</h2>
@using (Html.BeginForm(MVC.Report.Create(), FormMethod.Post, new { role = "form"}))
{
    @Html.Partial(MVC.Report.Views._CreateEdit , Model)
    <div class="row">
        <div class="col-sm-2"></div>
        <input type="submit" value="Create" class="btn btn-default" />
        @Html.ActionLink("Back To List", MVC.Report.Index(), new { @class = "btn btn-default" })
    </div>
}
