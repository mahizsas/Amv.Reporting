@model EditReportDetailsViewModel

@{
    ViewBag.Title = "Edit Report";
}
@section scripts{
    @Html.Partial(MVC.Report.Views._CommonJavaScript)
}

<h2>Edit Report</h2>
@using (Html.BeginForm(MVC.Report.Edit(), FormMethod.Post, new { role = "form" }))
{
    <div class="row">
        @Html.HiddenFor(m => m.Id)
        @Html.Partial(MVC.Report.Views._CreateEdit, Model)
    </div>
}
